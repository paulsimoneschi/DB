CREATE TABLE PLAYER (
	PLAYER_ID NUMBER(2),
	FIRST_NAME VARCHAR(12),
	LAST_NAME VARCHAR(14),
	POSITION VARCHAR(3) CHECK (POSITION IN('QB','RB','WR','TE','DB','SS','FS','SS','CB','DE','OLB')),
	TEAM_ID VARCHAR(3),
	TEAM_NAME VARCHAR(24),
	CONSTRAINT PLAYER_PLAYER_ID_PK PRIMARY KEY(PLAYER_ID),
	CONSTRAINT PLAYER_TEAM_ID_FK FOREIGN KEY (TEAM_ID) REFERENCES TEAM(TEAM_ID)
);
