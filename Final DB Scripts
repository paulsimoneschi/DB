CREATE TABLE DIVISION (
	DIVISION_ID NUMBER(2),
	DIVISION_NAME VARCHAR(10),
	CONSTRAINT DIVISION_DIVISION_ID_PK PRIMARY KEY(DIVISION_ID)
);
