CREATE TABLE GAME (
	GAME_ID NUMBER(3),
	AWAY_TEAM_ID VARCHAR(3),
	HOME_TEAM_ID VARCHAR(3),
	AWAY_TEAM_SCORE NUMBER(2) CHECK (AWAY_TEAM_SCORE BETWEEN 0 and 100),
	HOME_TEAM_SCORE NUMBER(2) CHECK (HOME_TEAM_SCORE BETWEEN 0 and 100),
	WEEK NUMBER(2) CHECK (WEEK BETWEEN 01 and 13),
	CONSTRAINT GAME_GAME_ID_PK PRIMARY KEY(GAME_ID),
	CONSTRAINT GAME_AWAY_TEAM_ID_FK FOREIGN KEY (AWAY_TEAM_ID) REFERENCES TEAM(TEAM_ID),
	CONSTRAINT GAME_HOME_TEAM_ID_FK FOREIGN KEY (HOME_TEAM_ID) REFERENCES TEAM(TEAM_ID)
);

